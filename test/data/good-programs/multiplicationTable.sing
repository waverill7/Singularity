$ Creates an empty matrix.
global multiplicationTable = [void]

$ Iterates through each row of the table.
for row = 1, row <= 10, row += 1:
    multiplicationTable[row-1] = [void]
    
    $ Iterates through each column of the table.
    for column = 1, column <= 10, column += 1:
        
        $ Fills in each entry of the table.
        multiplicationTable[row-1, column-1] = row * column
